<Window x:Class="MazeGenerator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MazeGenerator"
        mc:Ignorable="d"
        Title="Maze Generator" Height="450" Width="450" Icon="icon.png" Background="Gray" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="auto" />
        </Grid.RowDefinitions>
        <Canvas x:Name="myCanvas" Grid.Row="0" Width="420" Height="350" Background="White" />
        <Button x:Name="buttonGenerate" Grid.Row="1" Content="GENERATE" Width="75" Margin="8" Click="buttonGenerate_Click" />
    </Grid>
</Window>
